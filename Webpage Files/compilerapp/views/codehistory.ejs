<html>
	<head>
		<title> <%= title %> </title>
		<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css">
		<style>
			td {width: 50%;}
			td a {width: 100%; display: block;}
		</style>
		<script src="//code.jquery.com/jquery-1.11.3.min.js"></script>
		<script src="//code.jquery.com/jquery-migrate-1.2.1.min.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery.tablesorter/2.24.3/js/jquery.tablesorter.js"></script>
		<script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/js/bootstrap.min.js"></script>
	</head>

	<body>
	<table id="codehistory" class="table table-bordered table-hover tablesorter text-left">
		<thead>
			<tr>
				<th><span class="glyphicon glyphicon-list-alt"></span>&nbsp;Code</th>
				<th><span class="glyphicon glyphicon-ok"></span>&nbsp;Result</th>
			</tr>
		</thead>
		<tbody>

	    <% for (var i=0; i< result.pastAttempts.length; i++){%>
			<tr>
				<td>

					<p> <%= result.pastAttempts[i][0] %>  </p>
					
				</td>

				<td>
					<p> <%= result.pastAttempts[i][1] %> </p>
				</td>
			</tr>
		<%} %>

		</tbody>
	</table>
	</body>
</html>